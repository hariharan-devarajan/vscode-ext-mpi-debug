{
  "name": "mpi-debug",
  "displayName": "MPI Debug",
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mpi-debug.Attach to job",
        "title": "Attach To Job",
        "category": "MPI Debug"
      }
    ],
    "configuration": {
      "title": "MPI Debug",
      "properties": {
        "mpi-debug.gdbserver": {
          "description": "Specifies the path of gdbserver",
          "type": "string",
          "default": "gdbserver"
        }
      }
    }
  },
  "activationEvents": [
    "onCommand:mpi-debug.Attach to job"
  ],
  "dependencies": {
    "ws": "^8.4.0"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.58.0"
  },
  "publisher": "Hariharan",
  "license": "MIT License"
}